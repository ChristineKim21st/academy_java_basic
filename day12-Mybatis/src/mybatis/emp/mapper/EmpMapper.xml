<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="mybatis.emp.mapper.EmpMapper">
    <select id="selectAll" resultType="map">
        SELECT e.empno
             , e.ename
             , e.job
             , e.hiredate
             , e.comm
             , e.mgr
             , e.sal
             , e.deptno
          FROM emp e
    </select> 
</mapper>